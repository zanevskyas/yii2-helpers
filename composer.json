{
    "name": "zanevskyas/yii2-helpers",
    "description": "Kakadu helpers for Yii2",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "kakadu",
        "helpers"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ярмолюк Михаил",
            "email": "lukomi@mail.ru"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.1",
        "frostealth/yii2-aws-s3": "^2.1",
        "yiisoft/yii2-queue": "^2.0.0",
        "yiisoft/yii2-redis": "^2.0.0",
        "matthew-p/yii2-services": "^3.0",
        "kakadu-dev/yii2-base-helpers": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Kakadu\\Yii2Helpers\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}